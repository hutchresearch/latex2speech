<?xml version="1.0" encoding="UTF-8"?>

<ssmlfuncs>

    <!-- User Defined or Unsupported Function -->
    <userFunction>
        <funcname />
        <text>of</text>
        <repeat />
        <arg />
        <text>,</text>
    </userFunction>
    
    <!-- Equality -->
    <Equality>
        <arg />
        <text>equals</text>
        <arg />
    </Equality>

    <GreaterThan>
        <arg />
        <text>greater than or equal to</text>
